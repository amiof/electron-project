{
  "name": "electron",
  "version": "1.0.0",
  "description": "Electron App",
  "main": "./dist/main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc",
    "watch": "cross-env NODE_ENV=development tsc --watch",
    "start": "cross-env NODE_ENV=development tsc && cross-env NODE_ENV=development electronmon .",
    "start-lowlog-auto": "concurrently --prefix none -k \"npm run watch\" \"wait-on dist/preload.js && cross-env NODE_ENV=development electronmon .\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/ws": "^8.5.14",
    "dotenv": "^16.4.7",
    "electronmon": "^2.0.3",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@types/node": "^18.15.11",
    "@types/sqlite3": "^3.1.11",
    "cross-env": "^7.0.3",
    "electron": "^34.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "wait-on": "^8.0.2"
  }
}
